&share
 wrf_core = 'ARW',
 max_dom = 1,
 opt_output_from_geogrid_path = {{OUTPUT_PATH}}
/

&geogrid
 parent_id         = 1,
 parent_grid_ratio = 1,
 i_parent_start    = 1,
 j_parent_start    = 1,
 e_we              = {{E_WE}},
 e_sn              = {{E_SN}},
 geog_data_res = "default",
 dx = 6000,
 dy = 6000,
 map_proj = 'lambert',
 ref_lat   =  {{LAT}},
 ref_lon   =  {{LON}},
 truelat1  =  {{LAT}},
 stand_lon =  {{LON}},
 geog_data_path = {{DATA_PATH}}
/

